r\nimport CartContext from \"../../../helpers/cart\";\r\n\r\nconst ProductItem = ({ product, addCart, addWishlist, addCompareList }) => {\r\n  // eslint-disable-next-line\r\n  const router = useRouter();\r\n  const cartContext = useContext(CartContext);\r\n  const plusQty = cartContext.plusQty;\r\n  const minusQty = cartContext.minusQty;\r\n  const quantity = cartContext.quantity;\r\n  const setQuantity = cartContext.setQuantity;\r\n  const [image, setImage] = useState(\"\");\r\n  const [modal, setModal] = useState(false);\r\n  const [modalCompare, setModalCompare] = useState(false);\r\n  const toggleCompare = () => setModalCompare(!modalCompare);\r\n  const toggle = () => setModal(!modal);\r\n  const curContext = useContext(CurrencyContext);\r\n  const currency = curContext.state;\r\n  const uniqueTags = [];\r\n\r\n  const onClickHandle = (img) => {\r\n    setImage(img);\r\n  };\r\n\r\n  const changeQty = (e) => {\r\n    setQuantity(parseInt(e.target.value));\r\n  };\r\n\r\n  const clickProductDetail = () => {\r\n    const titleProps = product.title.split(\" \").join(\"\");\r\n    router.push(`/product-details/${product.id}` + \"-\" + `${titleProps}`);\r\n  };\r\n\r\n  const variantChangeByColor = (imgId, product_images) => {\r\n    product_images.map((data) => {\r\n      if (data.image_id == imgId) {\r\n        setImage(data.src);\r\n      }\r\n    });\r\n  };\r\n\r\n  let RatingStars = [];\r\n  for (var i = 0; i < product.rating; i++) {\r\n    RatingStars.push(<i className=\"fa fa-star\" key={i}></i>);\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <div className=\"product-box product-wrap\" key={i}>\r\n        <div className=\"img-wrapper\">\r\n          <div className=\"lable-block\">\r\n            {product.new === true ? <span className=\"lable3\">new</span> : \"\"}\r\n            {product.sale === true ? (\r\n              <span className=\"lable4\">on sale</span>\r\n            ) : (\r\n              \"\"\r\n            )}\r\n          </div>\r\n          <div className=\"front\" onClick={clickProductDetail}>\r\n            <Media\r\n              src={`${image ? image : product.images[0].src}`}\r\n              className=\"img-fluid\"\r\n              alt=\"\"\r\n            />\r\n          </div>\r\n          <div className=\"back\" onClick={clickProductDetail}>\r\n            <Media\r\n              src={`${image ? image : product.images[0].src}`}\r\n              className=\"img-fluid\"\r\n              alt=\"\"\r\n            />\r\n          </div>\r\n\r\n          {product.images[1] === \"undefined\" ? (\r\n            <div className=\"back\" onClick={clickProductDetail}>\r\n              <Media\r\n                src={`${image ? image : product.images[1].src}`}\r\n                className=\"img-fluid\"\r\n                alt=\"\"\r\n              />\r\n            </div>\r\n          ) : (\r\n            \"\"\r\n          )}\r\n\r\n          <div className=\"cart-detail\">\r\n            <a href={null} title=\"Add to Wishlist\" onClick={addWishlist}>\r\n              <i className=\"fa fa-heart\" aria-hidden=\"true\"></i>\r\n            </a>\r\n            <a\r\n              href={null}\r\n              data-toggle=\"modal\"\r\n              data-target=\"#quick-view\"\r\n              title=\"Quick View\"\r\n              onClick={toggle}\r\n            >\r\n              <i className=\"fa fa-search\" aria-hidden=\"true\"></i>\r\n            </a>\r\n            <a href={null} title=\"Compare\" onClick={toggleCompare}>\r\n              <i className=\"fa fa-refresh\" aria-hidden=\"true\"></i>\r\n            </a>\r\n            <Modal\r\n              isOpen={modalCompare}\r\n              toggle={toggleCompare}\r\n              size=\"lg\"\r\n              centered\r\n            >\r\n              <ModalHeader toggle={toggleCompare}>Quick View</ModalHeader>\r\n              <ModalBody>\r\n                <Row className=\"compare-modal\">\r\n                  <Col lg=\"12\">\r\n                    <div className=\"media\">\r\n                      <Media\r\n                        src={`${\r\n                          product.variants && image\r\n                            ? image\r\n                            : product.images[0].src\r\n                        }`}\r\n                        alt=\"\"\r\n                        className=\"img-fluid\"\r\n                      />\r\n                      <div className=\"media-body align-self-center text-center\">\r\n                        <h5>\r\n                          <i className=\"fa fa-check\"></i>Item{\" \"}\r\n                          <span>{product.title}</span>\r\n                          <span>successfully added to your Compare list</span>\r\n                        </h5>\r\n                        <div className=\"buttons d-flex justify-content-center\">\r\n                          <Link href=\"/page/compare\">\r\n                            <a\r\n                              href={null}\r\n                              className=\"btn-sm btn-solid\"\r\n                              onClick={addCompareList}\r\n                            >\r\n                              View Compare list\r\n                            </a>\r\n                          </Link>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </Col>\r\n                </Row>\r\n              </ModalBody>\r\n            </Modal>\r\n          </div>\r\n          {product.images ? (\r\n            <ul className=\"product-thumb-list\">\r\n              {product.images.map((img, i) => (\r\n                <li\r\n                  className={`grid_thumb_img ${\r\n                    img.src === image ? \"active\" : \"\"\r\n                  }`}\r\n                  key={i}\r\n                >\r\n                  <a href={null} title=\"Add to Wishlist\">\r\n                    <Media\r\n                      src={`${img.src}`}\r\n                      alt=\"wishlist\"\r\n                      onClick={() => onClickHandle(img.src)}\r\n                    />\r\n                  </a>\r\n                </li>\r\n              ))}\r\n            </ul>\r\n          ) : (\r\n            \"\"\r\n          )}\r\n        </div>\r\n        <div className=\"product-info\">\r\n          <div className=\"rating\">{RatingStars}</div>\r\n          <h6>{product.title}</h6>\r\n          <h4>\r\n            {currency.symbol}\r\n            {(\r\n              (product.price - (product.price * product.discount) / 100) *\r\n              currency.value\r\n            ).toFixed(2)}\r\n            <del>\r\n              <span className=\"money\">\r\n                {currency.symbol}\r\n                {(product.price * currency.value).toFixed(2)}\r\n              </span>\r\n            </del>\r\n          </h4>\r\n          {product.variants.map((vari) => {\r\n            var findItem = uniqueTags.find((x) => x.color === vari.color);\r\n            if (!findItem) uniqueTags.push(vari);\r\n          })}\r\n          {uniqueTags ? (\r\n            <ul className=\"color-variant\">\r\n              {uniqueTags.map((vari, i) => {\r\n                return (\r\n                  <li\r\n                    className={vari.color}\r\n                    key={i}\r\n                    title={vari.color}\r\n                    onClick={() =>\r\n                      variantChangeByColor(vari.image_id, product.images)\r\n                    }\r\n                  ></li>\r\n                );\r\n              })}\r\n            </ul>\r\n          ) : (\r\n            \"\"\r\n          )}\r\n          <div className=\"add-btn\">\r\n            <a href=\"#!\" onClick={addCart} className=\"btn btn-outline\">\r\n              <i className=\"fa fa-shopping-cart\"></i> add to cart\r\n            </a>\r\n          </div>\r\n        </div>\r\n        <Modal\r\n          isOpen={modal}\r\n          toggle={toggle}\r\n          className=\"quickview-modal\"\r\n          size=\"lg\"\r\n          centered\r\n        >\r\n          <ModalBody>\r\n            <Row>\r\n              <Col lg=\"6\" xs=\"12\">\r\n                <div className=\"quick-view-img\">\r\n                  <Media\r\n                    src={`${\r\n                      product.variants && image ? image : product.images[0].src\r\n                    }`}\r\n                    alt=\"\"\r\n                    className=\"img-fluid\"\r\n                  />\r\n                </div>\r\n              </Col>\r\n              <Col lg=\"6\" className=\"rtl-text\">\r\n                <div className=\"product-right\">\r\n                  <h2> {product.title} </h2>\r\n                  <h3>\r\n                    {currency.symbol}\r\n                    {(product.price * currency.value).toFixed(2)}\r\n                  </h3>\r\n                  {product.variants ? (\r\n                    <ul className=\"color-variant\">\r\n                      {uniqueTags ? (\r\n                        <ul className=\"color-variant\">\r\n                          {uniqueTags.map((vari, i) => {\r\n                            return (\r\n                              <li\r\n                                className={vari.color}\r\n                                key={i}\r\n                                title={vari.color}\r\n                                onClick={() =>\r\n                                  variantChangeByColor(\r\n                                    vari.image_id,\r\n                                    product.images\r\n                                  )\r\n                                }\r\n                              ></li>\r\n                            );\r\n                          })}\r\n                        </ul>\r\n                      ) : (\r\n                        \"\"\r\n                      )}\r\n                    </ul>\r\n                  ) : (\r\n                    \"\"\r\n                  )}\r\n                  <div className=\"border-product\">\r\n                    <h6 className=\"product-title\">product details</h6>\r\n                    <p>{product.description}</p>\r\n                  </div>\r\n                  <div className=\"product-description border-product\">\r\n                    {product.size ? (\r\n                      <div className=\"size-box\">\r\n                        <ul>\r\n                          {product.size.map((size, i) => {\r\n                            return (\r\n                              <li key={i}>\r\n                                <a href={null}>{size}</a>\r\n                              </li>\r\n                            );\r\n                          })}\r\n                        </ul>\r\n                      </div>\r\n                    ) : (\r\n                      \"\"\r\n                    )}\r\n                    <h6 className=\"product-title\">quantity</h6>\r\n                    <div className=\"qty-box\">\r\n                      <div className=\"input-group\">\r\n                        <span className=\"input-group-prepend\">\r\n                          <button\r\n                            type=\"button\"\r\n                            className=\"btn quantity-left-minus\"\r\n                            onClick={minusQty}\r\n                            data-type=\"minus\"\r\n                            data-field=\"\"\r\n                          >\r\n                            <i className=\"fa fa-angle-left\"></i>\r\n                          </button>\r\n                        </span>\r\n                        <input\r\n                          type=\"text\"\r\n                          name=\"quantity\"\r\n                          value={quantity}\r\n                          onChange={changeQty}\r\n                          className=\"form-control input-number\"\r\n                        />\r\n                        <span className=\"input-group-prepend\">\r\n                          <button\r\n                            type=\"button\"\r\n                            className=\"btn quantity-right-plus\"\r\n                            onClick={() => plusQty(product)}\r\n                            data-type=\"plus\"\r\n                            data-field=\"\"\r\n                          >\r\n                            <i className=\"fa fa-angle-right\"></i>\r\n                          </button>\r\n                        </span>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"product-buttons\">\r\n                    <button\r\n                      className=\"btn btn-solid\"\r\n                      onClick={() => addCart(product, quantity)}\r\n                    >\r\n                      add to cart\r\n                    </button>\r\n                    <button\r\n                      className=\"btn btn-solid\"\r\n                      onClick={clickProductDetail}\r\n                    >\r\n                      View detail\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </Col>\r\n            </Row>\r\n          </ModalBody>\r\n        </Modal>\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductItem;\r\n"]},"metadata":{},"sourceType":"module"}
\r\n        )}\r\n\r\n        <div className={cartClass}>\r\n          <button title=\"Add to cart\" onClick={addCart}>\r\n            <i className=\"fa fa-shopping-cart\" aria-hidden=\"true\"></i>\r\n          </button>\r\n          <a href={null} title=\"Add to Wishlist\" onClick={addWishlist}>\r\n            <i className=\"fa fa-heart\" aria-hidden=\"true\"></i>\r\n          </a>\r\n          <a href={null} title=\"Quick View\" onClick={toggle}>\r\n            <i className=\"fa fa-search\" aria-hidden=\"true\"></i>\r\n          </a>\r\n          <a href={null} title=\"Compare\" onClick={toggleCompare}>\r\n            <i className=\"fa fa-refresh\" aria-hidden=\"true\"></i>\r\n          </a>\r\n          <Modal isOpen={modalCompare} toggle={toggleCompare} centered>\r\n            <ModalHeader toggle={toggleCompare}>Quick View</ModalHeader>\r\n            <ModalBody>\r\n              <Row className=\"compare-modal\">\r\n                <Col lg=\"12\">\r\n                  <div className=\"media\">\r\n                    <Media\r\n                      src={`${\r\n                        product.variants && image\r\n                          ? image\r\n                          : product.images[0].src\r\n                      }`}\r\n                      alt=\"\"\r\n                      className=\"img-fluid\"\r\n                    />\r\n                    <div className=\"media-body align-self-center text-center\">\r\n                      <h5>\r\n                        <i className=\"fa fa-check\"></i>Item{\" \"}\r\n                        <span>{product.title}</span>\r\n                        <span>successfully added to your Compare list</span>\r\n                      </h5>\r\n                      <div className=\"buttons d-flex justify-content-center\">\r\n                        <Link href=\"/page/compare\">\r\n                          <a\r\n                            href={null}\r\n                            className=\"btn-sm btn-solid\"\r\n                            onClick={addToCompare}\r\n                          >\r\n                            View Compare list\r\n                          </a>\r\n                        </Link>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </Col>\r\n              </Row>\r\n            </ModalBody>\r\n          </Modal>\r\n        </div>\r\n        {product.images ? (\r\n          <ul className=\"product-thumb-list\">\r\n            {product.images.map((img, i) => (\r\n              <li\r\n                className={`grid_thumb_img ${\r\n                  img.src === image ? \"active\" : \"\"\r\n                }`}\r\n                key={i}\r\n              >\r\n                <a href={null} title=\"Add to Wishlist\">\r\n                  <Media\r\n                    src={`${img.src}`}\r\n                    alt=\"wishlist\"\r\n                    onClick={() => onClickHandle(img.src)}\r\n                  />\r\n                </a>\r\n              </li>\r\n            ))}\r\n          </ul>\r\n        ) : (\r\n          \"\"\r\n        )}\r\n      </div>\r\n      <MasterProductDetail\r\n        product={product}\r\n        productDetail=\"\"\r\n        currency={currency}\r\n        uniqueTags={uniqueTags}\r\n        variantChangeByColor={variantChangeByColor}\r\n        detailClass=\"text-center\"\r\n      />\r\n      <Modal\r\n        isOpen={modal}\r\n        toggle={toggle}\r\n        className=\"modal-lg quickview-modal\"\r\n        centered\r\n        size=\"lg\"\r\n      >\r\n        <ModalBody>\r\n          <Row>\r\n            <Col lg=\"6\" xs=\"12\">\r\n              <div className=\"quick-view-img\">\r\n                <Media\r\n                  src={`${\r\n                    product.variants && image ? image : product.images[0].src\r\n                  }`}\r\n                  alt=\"\"\r\n                  className=\"img-fluid\"\r\n                />\r\n              </div>\r\n            </Col>\r\n            <Col lg=\"6\" className=\"rtl-text\">\r\n              <div className=\"product-right\">\r\n                <h2> {product.title} </h2>\r\n                <h3>\r\n                  {currency.symbol}\r\n                  {(product.price * currency.value).toFixed(2)}\r\n                </h3>\r\n                {product.variants ? (\r\n                  <ul className=\"color-variant\">\r\n                    {uniqueTags ? (\r\n                      <ul className=\"color-variant\">\r\n                        {uniqueTags.map((vari, i) => {\r\n                          return (\r\n                            <li\r\n                              className={vari.color}\r\n                              key={i}\r\n                              title={vari.color}\r\n                              onClick={() =>\r\n                                variantChangeByColor(\r\n                                  vari.image_id,\r\n                                  product.images\r\n                                )\r\n                              }\r\n                            ></li>\r\n                          );\r\n                        })}\r\n                      </ul>\r\n                    ) : (\r\n                      \"\"\r\n                    )}\r\n                  </ul>\r\n                ) : (\r\n                  \"\"\r\n                )}\r\n                <div className=\"border-product\">\r\n                  <h6 className=\"product-title\">product details</h6>\r\n                  <p>{product.description}</p>\r\n                </div>\r\n                <div className=\"product-description border-product\">\r\n                  {product.size ? (\r\n                    <div className=\"size-box\">\r\n                      <ul>\r\n                        {product.size.map((size, i) => {\r\n                          return (\r\n                            <li key={i}>\r\n                              <a href={null}>{size}</a>\r\n                            </li>\r\n                          );\r\n                        })}\r\n                      </ul>\r\n                    </div>\r\n                  ) : (\r\n                    \"\"\r\n                  )}\r\n                  <h6 className=\"product-title\">quantity</h6>\r\n                  <div className=\"qty-box\">\r\n                    <div className=\"input-group\">\r\n                      <span className=\"input-group-prepend\">\r\n                        <button\r\n                          type=\"button\"\r\n                          className=\"btn quantity-left-minus\"\r\n                          onClick={minusQty}\r\n                          data-type=\"minus\"\r\n                          data-field=\"\"\r\n                        >\r\n                          <i className=\"fa fa-angle-left\"></i>\r\n                        </button>\r\n                      </span>\r\n                      <input\r\n                        type=\"text\"\r\n                        name=\"quantity\"\r\n                        value={quantity}\r\n                        onChange={changeQty}\r\n                        className=\"form-control input-number\"\r\n                      />\r\n                      <span className=\"input-group-prepend\">\r\n                        <button\r\n                          type=\"button\"\r\n                          className=\"btn quantity-right-plus\"\r\n                          onClick={() => plusQty(product)}\r\n                          data-type=\"plus\"\r\n                          data-field=\"\"\r\n                        >\r\n                          <i className=\"fa fa-angle-right\"></i>\r\n                        </button>\r\n                      </span>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div className=\"product-buttons\">\r\n                  <button\r\n                    className=\"btn btn-solid\"\r\n                    
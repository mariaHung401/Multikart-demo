      discount\r\n        variants {\r\n          id\r\n          sku\r\n          size\r\n          color\r\n          image_id\r\n        }\r\n        images {\r\n          image_id\r\n          id\r\n          alt\r\n          src\r\n        }\r\n      }\r\n    }\r\n  }\r\n`;\r\n\r\nconst TabContent = ({ data, loading, cartClass, startIndex, endIndex }) => {\r\n  const context = useContext(CartContext);\r\n  const wishListContext = useContext(WishlistContext);\r\n  const compareContext = useContext(CompareContext);\r\n  const quantity = context.quantity;\r\n  return (\r\n    <div>\r\n      {!data ||\r\n      !data.products ||\r\n      !data.products.items ||\r\n      data.products.items.length === 0 ||\r\n      loading ? (\r\n        <div className=\"row mx-0 margin-default\">\r\n          <div className=\"col-xl-3 col-lg-4 col-6\">\r\n            <PostLoader />\r\n          </div>\r\n          <div className=\"col-xl-3 col-lg-4 col-6\">\r\n            <PostLoader />\r\n          </div>\r\n          <div className=\"col-xl-3 col-lg-4 col-6\">\r\n            <PostLoader />\r\n          </div>\r\n          <div className=\"col-xl-3 col-lg-4 col-6\">\r\n            <PostLoader />\r\n          </div>\r\n        </div>\r\n      ) : (\r\n        <Slider {...Product4} className=\"product-4 product-m no-arrow\">\r\n          {data &&\r\n            data.products.items\r\n              .slice(startIndex, endIndex)\r\n              .map((product, i) => (\r\n                <ProductItem\r\n                  product={product}\r\n                  addCart={() => context.addToCart(product, quantity)}\r\n                  key={i}\r\n                  addWishlist={() => wishListContext.addToWish(product)}\r\n                  addCompareList={() => compareContext.addToCompare(product)}\r\n                  cartClass={cartClass}\r\n                />\r\n              ))}\r\n        </Slider>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nconst TabCollection = ({ type, cartClass, designClass }) => {\r\n  const [activeTab, setActiveTab] = useState(type);\r\n\r\n  var { loading, data } = useQuery(GET_PRODUCTS, {\r\n    variables: {\r\n      type: activeTab,\r\n      indexFrom: 0,\r\n      limit: 32,\r\n    },\r\n  });\r\n\r\n  return (\r\n    <section className={designClass}>\r\n      <Container>\r\n        <div className=\"title2\">\r\n          <h4>new collection</h4>\r\n          <h2 className=\"title-inner2\">trending products</h2>\r\n        </div>\r\n        <Row>\r\n          <Col>\r\n            <Tabs className=\"theme-tab\">\r\n              <TabList className=\"tabs tab-title\">\r\n                <Tab\r\n                  className={activeTab == type ? \"active\" : \"\"}\r\n                  onClick={() => setActiveTab(type)}\r\n                >\r\n                  new arrival\r\n                </Tab>\r\n                <Tab\r\n                  className={activeTab == type ? \"active\" : \"\"}\r\n                  onClick={() => setActiveTab(type)}\r\n                >\r\n                  woman\r\n                </Tab>\r\n                <Tab\r\n                  className={activeTab == type ? \"active\" : \"\"}\r\n                  onClick={() => setActiveTab(type)}\r\n                >\r\n                  man\r\n                </Tab>\r\n                <Tab\r\n                  className={activeTab == \"kids\" ? \"active\" : \"\"}\r\n                  onClick={() => setActiveTab(\"kids\")}\r\n                >\r\n                  kids\r\n                </Tab>\r\n                <Tab\r\n                  className={activeTab == \"bags\" ? \"active\" : \"\"}\r\n                  onClick={() => setActiveTab(\"bags\")}\r\n                >\r\n                  school-bag\r\n                </Tab>\r\n              </TabList>\r\n              <div className=\"tab-content-cls\">\r\n                <TabPanel className=\"tab-content active default\">\r\n                  <TabContent\r\n                    data={data}\r\n                    cartClass={cartClass}\r\n                    loading={loading}\r\n                  />\r\n                </TabPanel>\r\n                <TabPanel className=\"tab-content\">\r\n                  <TabContent\r\n                    data={data}\r\n                    cartClass={cartClass}\r\n                    loading={loading}\r\n                    startIndex={8}\r\n                    endIndex={16}\r\n                  />\r\n                  <TabContent />\r\n                </TabPanel>\r\n                <TabPanel className=\"tab-content\">\r\n                  <TabContent\r\n                    data={data}\r\n                    cartClass={cartClass}\r\n                    loading={loading}\r\n                    startIndex={16}\r\n                    endIndex={32}\r\n                  />\r\n                  <TabContent />\r\n                </TabPanel>\r\n                <TabPanel className=\"tab-content\">\r\n                  <TabContent\r\n                    data={data}\r\n                    cartClass={cartClass}\r\n                    loading={loading}\r\n                    startIndex={0}\r\n                    endIndex={8}\r\n                  />\r\n                  <TabContent />\r\n                </TabPanel>\r\n                <TabPanel className=\"tab-content\">\r\n                  <TabContent\r\n                    data={data}\r\n                    cartClass={cartClass}\r\n                    loading={loading}\r\n                    startIndex={0}\r\n                    endIndex={8}\r\n                  />\r\n                  <TabContent />\r\n                </TabPanel>\r\n              </div>\r\n            </Tabs>\r\n          </Col>\r\n        </Row>\r\n      </Container>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default TabCollection;\r\n"]},"metadata":{},"sourceType":"module"}